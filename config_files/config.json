{
  "paths": {
     "executables": {
        "dlv": "executables/dlv/dlv",
        "clingo": "executables/clingo/clingo",
        "dlv2": "executables/dlv2/dlv2",
        "idlv": "executables/idlv/idlv",
        "timeout": "executables/timeout/timeout"
    },
    "certificate": {
      "cert_file": "cert_file/cert.crt",
      "key_file": "cert_file/cert.key"
    }
  },
  "server_properties": {
    "port_number" : "443",
    "cors_origins" : ["localhost"]
  },
  "output": {
      "max_chars": 10000
  },
  "limits": {
      "time": 20,
      "memory": 200000
  },
  "available_options": {
      "dlv": ["", "free choice", "-silent", "-filter=", "-nofacts", "-FC"],
      "clingo": ["", "free choice"],
      "dlv2": ["", "free choice"],
      "idlv": ["", "free choice"]
   }
}

